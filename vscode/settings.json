{
  "C_Cpp.clang_format_fallbackStyle": "Google",
  "C_Cpp.clang_format_sortIncludes": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "advancedNewFile.convenienceOptions": ["root", "current", "last"],
  "aws.profile": "profile:default",
  "bookmarks.label.suggestion": "useWhenSelected",
  "bookmarks.saveBookmarksInProject": true,
  "bookmarks.sideBar.expanded": true,
  "breadcrumbs.enabled": true,
  "cSpell.allowCompoundWords": true,
  "cSpell.dictionaries": ["customTerms"],
  "cSpell.dictionaryDefinitions": [
    {
      "name": "customTerms",
      "path": "~/Repos/src/github.com/uetchy/dotfiles/vscode/cspell.txt"
    }
  ],
  "cSpell.userWords": [],
  "code-runner.runInTerminal": true,
  "debug.console.closeOnEnd": true,
  "debug.console.historySuggestions": false,
  "debug.inlineValues": true,
  "debug.internalConsoleOptions": "openOnSessionStart",
  "editor.acceptSuggestionOnEnter": "off",
  "editor.fontFamily": "JetBrains Mono, Cascadia Code, Source Han Code JP Bold, monospace",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.hover.delay": 1000,
  "editor.lineNumbers": "interval",
  "editor.minimap.renderCharacters": false,
  "editor.padding.top": 40,
  "editor.quickSuggestions": {
    "comments": false,
    "other": true,
    "strings": true
  },
  "editor.renderLineHighlight": "all",
  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "deepIndent",
  "eslint.nodePath": "/usr/local/bin/node",
  "eslint.packageManager": "yarn",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "explorer.sortOrder": "type",
  "extensions.ignoreRecommendations": true,
  "favorites.storageFilePath": ".vscode/favorites.json",
  "files.associations": {
    "*.json": "jsonc",
    ".all-contributorsrc": "json",
    ".direnvrc": "shellscript",
    ".envrc": "shellscript",
    ".textlintrc": "jsonc",
    "zshrc": "shellscript"
  },
  "files.exclude": {
    "**/.cache": true,
    "**/.venv": true,
    "**/__pycache__": true,
    "**/node_modules": true,
    "**/package-lock.json": true,
    "**/poetry.lock": true,
    "**/yarn.lock": true,
    "*.egg-info": true,
    ".nyc_output": true,
    ".pytest_cache": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/.venv/**": true,
    "**/node_modules/**": true
  },
  "git-graph.showStatusBarItem": false,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.rebaseWhenSync": true,
  "githubIssues.queries": [
    {
      "label": "Created Issues",
      "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
    },
    {
      "label": "All",
      "query": "state:open repo:${owner}/${repository} sort:created-desc"
    }
  ],
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "hediet.vscode-drawio.local-storage": "",
  "html.autoClosingTags": false,
  "javascript.autoClosingTags": false,
  "javascript.preferences.useAliasesForRenames": false,
  "material-icon-theme.activeIconPack": "react_redux",
  "material-icon-theme.files.associations": {
    "*.LICENSE.txt": "certificate",
    "*.paw": "http",
    ".envrc": "exe",
    ".swiftformat": "swift",
    "tsconfig.json": "typescript-def",
    "tslint.json": "typescript-def"
  },
  "material-icon-theme.folders.color": "#8b8a8a",
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.showReloadMessage": false,
  "material-icon-theme.showWelcomeMessage": false,
  "materialTheme.accent": "Indigo",
  "npm.packageManager": "yarn",
  "openInGitHub.useLocalLine": true,
  "platformio-ide.activateOnlyOnPlatformIOProject": true,
  "prettier.packageManager": "yarn",
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "python.autoComplete.addBrackets": true,
  "python.dataScience.askForKernelRestart": false,
  "python.dataScience.sendSelectionToInteractiveWindow": true,
  "python.diagnostics.sourceMapsEnabled": true,
  "python.formatting.provider": "yapf",
  "python.formatting.yapfArgs": [
    "--style={based_on_style: google, indent_width: 2}"
  ],
  "python.formatting.yapfPath": "/usr/local/bin/yapf",
  "python.languageServer": "Microsoft",
  "python.linting.banditEnabled": false,
  "python.linting.flake8Enabled": false,
  "python.linting.pylintEnabled": true,
  "python.pythonPath": "/usr/local/bin/python3",
  "python.terminal.activateEnvironment": false,
  "ruby.format": "rufo",
  "ruby.useBundler": true,
  "ruby.useLanguageServer": true,
  "scm.defaultViewMode": "tree",
  "scm.diffDecorations": "gutter",
  "scm.diffDecorationsGutterWidth": 1,
  "search.exclude": {
    ".venv": true
  },
  "search.seedOnFocus": true,
  "shortcuts.buttons": [
    "beaker , workbench.action.tasks.test , Run Test",
    "bug, debugLauncher.auto, Debug Launcher (Auto)",
    "terminal , workbench.action.terminal.openNativeConsole, Open in Terminal",
    "file-directory , openInFinder.openRoot , Open in Finder",
    "git-merge , openInGitFork.open , Open in Fork"
  ],
  "tabnine.experimentalAutoImports": true,
  "taskExplorer.enableBatch": false,
  "taskExplorer.enableSideBar": true,
  "taskExplorer.pathToPython": "python3",
  "taskExplorer.showLastTasks": false,
  "taskExplorer.showRunningTask": false,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.enableBell": true,
  "terminal.integrated.fontFamily": "Menlo for Powerline, Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.lineHeight": 1.3,
  "terminal.integrated.minimumContrastRatio": 4.5,
  "todo.file.exclude": [
    "**/.!(todo|todos|task|tasks)/**",
    "**/_output/**",
    "**/bower_components/**",
    "**/build/**",
    "**/dist/**",
    "**/node_modules/**",
    "**/out/**",
    "**/output/**",
    "**/release/**",
    "**/releases/**",
    "**/static/**",
    "**/target/**",
    "**/third_party/**",
    "**/vendor/**",
    "**/.venv/**"
  ],
  "typescript.autoClosingTags": false,
  "typescript.preferences.useAliasesForRenames": false,
  "typescript.suggest.paths": false,
  "vsintellicode.features.python.deepLearning": "enabled",
  "vsintellicode.modify.editor.suggestSelection": "choseToUpdateConfiguration",
  "window.closeWhenEmpty": true,
  "window.newWindowDimensions": "maximized",
  "window.openFilesInNewWindow": "default",
  "workbench.colorTheme": "GitHub Dark",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.tabSizing": "shrink",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "welcomePageInEmptyWorkbench",
  "workbench.tree.indent": 10
}
