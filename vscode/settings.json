{
  "C_Cpp.clang_format_fallbackStyle": "Google",
  "C_Cpp.clang_format_sortIncludes": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "advancedNewFile.convenienceOptions": ["root", "current", "last"],
  "auto-close-tag.SublimeText3Mode": true,
  "aws.profile": "profile:default",
  "bookmarks.saveBookmarksInProject": true,
  "bookmarks.sideBar.expanded": true,
  "breadcrumbs.enabled": true,
  "browser-preview.startUrl": "https://ddg.gg",
  "cSpell.userWords": [
    "EINTERNAL",
    "ENOTFOUND",
    "EPUB",
    "ESNEXT",
    "Frontmatter",
    "Pluggable",
    "Repos",
    "SVGs",
    "Uechi",
    "Vivliostyle",
    "WCAG",
    "Yasuaki",
    "dtype",
    "entrypoint",
    "forbital",
    "gcloud",
    "globby",
    "glyf",
    "grammarly",
    "hparam",
    "hparams",
    "katex",
    "lstm",
    "maglev",
    "mathjax",
    "matmul",
    "mdast",
    "numpy",
    "octokit",
    "pretrained",
    "pylint",
    "randn",
    "ratelimit",
    "rehype",
    "sess",
    "sourcemap",
    "tfrecord",
    "tslib",
    "typechecking",
    "uetchy",
    "unist",
    "venv",
    "vfile"
  ],
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.fontFamily": "JetBrains Mono, Cascadia Code, Source Han Code JP Bold, monospace",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.hover.delay": 500,
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.padding.top": 40,
  "editor.parameterHints.enabled": true,
  "editor.quickSuggestions": {
    "comments": false,
    "other": true,
    "strings": true
  },
  "editor.renderLineHighlight": "line",
  "editor.renderWhitespace": "selection",
  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "deepIndent",
  "eslint.nodePath": "/usr/local/bin/node",
  "eslint.packageManager": "yarn",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "favorites.storageFilePath": ".vscode/favorites.json",
  "files.associations": {
    ".envrc": "shellscript",
    ".textlintrc": "json",
    "zshrc": "shellscript"
  },
  "files.exclude": {
    "**/.cache": true,
    "**/.venv": true,
    "**/__pycache__": true,
    "**/node_modules": true,
    "**/package-lock.json": true,
    "**/poetry.lock": true,
    "**/yarn.lock": true,
    "*.egg-info": true,
    ".nyc_output": true,
    ".pytest_cache": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.venv/**": true
  },
  "findrelated.applyRulesets": ["c/c++", "csharp", "jest"],
  "findrelated.rulesets": [
    {
      "name": "jest",
      "rules": [
        {
          "locators": ["$1.test.{t,j}sx", "$1.test.{t,j}s"],
          "pattern": "(.+)\\.[tj]sx?$"
        },
        {
          "locators": ["$1.{t,j}sx", "$1.{t,j}s"],
          "pattern": "(.+)\\.test\\.[tj]sx?$"
        }
      ]
    }
  ],
  "git-graph.showStatusBarItem": false,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.rebaseWhenSync": true,
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "grammarly.autoActivate": false,
  "grammarly.dialect": "british",
  "grammarly.userWords": ["emmake", "vivliostyle"],
  "hediet.vscode-drawio.local-storage": "",
  "html.autoClosingTags": false,
  "javascript.autoClosingTags": false,
  "javascript.preferences.useAliasesForRenames": false,
  "material-icon-theme.activeIconPack": "react_redux",
  "material-icon-theme.files.associations": {
    "*.paw": "http",
    ".envrc": "exe",
    ".swiftformat": "swift",
    "tsconfig.json": "typescript-def",
    "tslint.json": "typescript-def"
  },
  "material-icon-theme.folders.color": "#8b8a8a",
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.showReloadMessage": false,
  "material-icon-theme.showWelcomeMessage": false,
  "materialTheme.accent": "Indigo",
  "npm.packageManager": "yarn",
  "platformio-ide.activateOnlyOnPlatformIOProject": true,
  "prettier.arrowParens": "always",
  "prettier.packageManager": "yarn",
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "python.analysis.memory.keepLibraryAst": true,
  "python.autoComplete.addBrackets": true,
  "python.dataScience.askForKernelRestart": false,
  "python.dataScience.sendSelectionToInteractiveWindow": true,
  "python.formatting.provider": "yapf",
  "python.formatting.yapfPath": "/usr/local/bin/yapf",
  "python.languageServer": "Microsoft",
  "python.linting.banditEnabled": false,
  "python.linting.flake8Enabled": false,
  "python.linting.pylintEnabled": true,
  "python.pythonPath": "/usr/local/bin/python3",
  "ruby.format": "rufo",
  "ruby.useBundler": true,
  "ruby.useLanguageServer": true,
  "scm.defaultViewMode": "tree",
  "scm.diffDecorations": "gutter",
  "scm.diffDecorationsGutterVisibility": "hover",
  "search.exclude": {
    ".venv": true
  },
  "search.seedOnFocus": true,
  "shortcuts.buttons": [
    "beaker , workbench.action.tasks.test , Run Test",
    "bug, debugLauncher.auto, Debug Launcher (Auto)",
    "terminal , workbench.action.terminal.openNativeConsole, Open in Terminal",
    "file-directory , openInFinder.openRoot , Open in Finder",
    "git-merge , openInGitFork.open , Open in Fork"
  ],
  "swiftformat.configSearchPaths": [".swiftformat"],
  "tabnine.experimentalAutoImports": false,
  "taskExplorer.enableBatch": false,
  "taskExplorer.enableExplorerView": false,
  "taskExplorer.enablePowershell": false,
  "taskExplorer.enableSideBar": true,
  "taskExplorer.pathToPython": "python3",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.enableBell": true,
  "terminal.integrated.fontFamily": "Menlo for Powerline, Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.lineHeight": 1.3,
  "todo.file.exclude": [
    "**/.!(todo|todos|task|tasks)/**",
    "**/_output/**",
    "**/bower_components/**",
    "**/build/**",
    "**/dist/**",
    "**/node_modules/**",
    "**/out/**",
    "**/output/**",
    "**/release/**",
    "**/releases/**",
    "**/static/**",
    "**/target/**",
    "**/third_party/**",
    "**/vendor/**",
    "**/.venv/**"
  ],
  "tslint.packageManager": "yarn",
  "typescript.autoClosingTags": false,
  "typescript.preferences.useAliasesForRenames": false,
  "typescript.suggest.paths": false,
  "vsicons.dontShowNewVersionMessage": true,
  "vsintellicode.features.python.deepLearning": "enabled",
  "vsintellicode.modify.editor.suggestSelection": "choseToUpdateConfiguration",
  "window.closeWhenEmpty": true,
  "window.newWindowDimensions": "maximized",
  "window.openFilesInNewWindow": "default",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Material Theme Palenight",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.tabCloseButton": "off",
  "workbench.editor.tabSizing": "shrink",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.panel.defaultLocation": "right",
  "workbench.startupEditor": "welcomePageInEmptyWorkbench",
  "workbench.tree.indent": 15
}
