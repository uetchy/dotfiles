{
  "C_Cpp.clang_format_fallbackStyle": "Google",
  "C_Cpp.clang_format_sortIncludes": true,
  "[dockerfile]": {
    "editor.defaultFormatter": "PeterJausovec.vscode-docker"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "breadcrumbs.enabled": true,
  "csharp.suppressDotnetInstallWarning": true,
  "diffEditor.renderSideBySide": false,
  "editor.codeActionsOnSave": {
    "source.organizeImports": false
  },
  "editor.fontFamily": "Source Code Pro, Fira Code, monospace",
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.parameterHints.enabled": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "eslint.nodePath": "/usr/local/bin/node",
  "eslint.packageManager": "yarn",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    ".envrc": "shellscript",
    "zshrc": "shellscript"
  },
  "files.exclude": {
    "**/node_modules": false,
    ".nyc_output": true,
    ".venv": true,
    ".direnv": true,
    "package-lock.json": true,
    "yarn.lock": true,
    "poetry.lock": true,
    ".pytest_cache": true,
    "*.egg-info": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.rebaseWhenSync": true,
  "gitlens.advanced.telemetry.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.currentLine.enabled": false,
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.showReloadMessage": false,
  "material-icon-theme.showWelcomeMessage": false,
  "platformio-ide.activateOnlyOnPlatformIOProject": true,
  "prettier.arrowParens": "always",
  "prettier.eslintIntegration": true,
  "prettier.jsxBracketSameLine": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "python.autoComplete.addBrackets": true,
  "python.dataScience.sendSelectionToInteractiveWindow": true,
  "python.formatting.provider": "yapf",
  "python.formatting.yapfArgs": [
    "--style",
    "{based_on_style: chromium, indent_width: 4, column_limit: 120}"
  ],
  "python.formatting.yapfPath": "/usr/local/bin/yapf",
  "python.jediEnabled": false,
  "python.linting.banditEnabled": false,
  "python.linting.banditPath": "/usr/local/bin/bandit",
  "python.linting.flake8Args": [
    "--max-line-length=120",
    "--ignore=E111,E114,E501,W504,E126"
  ],
  "python.linting.flake8Enabled": false,
  "python.linting.flake8Path": "/usr/local/bin/flake8",
  "python.linting.pylintEnabled": true,
  "python.linting.pylintPath": "",
  "python.pipenvPath": "/usr/local/bin/pipenv",
  "python.pythonPath": "/usr/local/bin/python3",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.enableBell": true,
  "terminal.integrated.fontFamily": "Menlo for Powerline, Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 13,
  "vsintellicode.modify.editor.suggestSelection": "choseToUpdateConfiguration",
  "window.newWindowDimensions": "inherit",
  "window.openFilesInNewWindow": "default",
  "workbench.colorTheme": "Code Blue",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.openDefaultSettings": true,
  "workbench.statusBar.feedback.visible": false,
  "editor.hover.delay": 1500,
  "editor.wordWrap": "on",
  "leetcode.defaultLanguage": "python3",
  "leetcode.enableStatusBar": false,
  "npm.packageManager": "yarn",
  "taskExplorer.enableAnt": false,
  "taskExplorer.enableAnsiconForAnt": false,
  "taskExplorer.enableBatch": false,
  "taskExplorer.enableNsis": false,
  "taskExplorer.enablePerl": false,
  "taskExplorer.enablePowershell": false,
  "taskExplorer.enableSideBar": true,
  "taskExplorer.pathToPython": "python3",
  "workbench.sideBar.location": "left",
  "window.zoomLevel": 0,
  "editor.foldingStrategy": "indentation",
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.views.compare.avatars": false
}
