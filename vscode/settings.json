{
  "C_Cpp.clang_format_fallbackStyle": "Google",
  "C_Cpp.clang_format_sortIncludes": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[makefile]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 4
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "advancedNewFile.convenienceOptions": ["root", "current", "last"],
  "bookmarks.label.suggestion": "useWhenSelected",
  "bookmarks.saveBookmarksInProject": true,
  "bookmarks.sideBar.expanded": true,
  "cSpell.allowCompoundWords": true,
  "cSpell.dictionaries": ["customTerms"],
  "cSpell.dictionaryDefinitions": [
    {
      "name": "customTerms",
      "path": "~/Repos/src/github.com/uetchy/dotfiles/vscode/cspell.txt"
    }
  ],
  "cSpell.userWords": [],
  "code-runner.enableAppInsights": false,
  "code-runner.runInTerminal": true,
  "customizeUI.activityBar": "bottom",
  "customizeUI.titleBar": "inline",
  "debug.console.closeOnEnd": false,
  "debug.console.historySuggestions": false,
  "debug.inlineValues": true,
  "debug.internalConsoleOptions": "openOnSessionStart",
  "debug.javascript.usePreview": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.cursorBlinking": "phase",
  "editor.cursorStyle": "line-thin",
  "editor.fontFamily": "JetBrains Mono, Cascadia Code, Source Han Code JP Bold, monospace",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.hover.delay": 1000,
  "editor.lineNumbers": "interval",
  "editor.minimap.renderCharacters": false,
  "editor.padding.top": 50,
  "editor.renderIndentGuides": false,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "trailing",
  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "deepIndent",
  "eslint.nodePath": "/usr/local/bin/node",
  "eslint.packageManager": "yarn",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.colors": false,
  "explorer.openEditors.visible": 0,
  "explorer.sortOrder": "type",
  "extensions.ignoreRecommendations": true,
  "favorites.storageFilePath": ".vscode/favorites.json",
  "files.associations": {
    "*.json": "jsonc",
    "*.rules": "yaml",
    ".all-contributorsrc": "json",
    ".direnvrc": "shellscript",
    ".envrc": "shellscript",
    ".fixpackrc": "jsonc",
    ".prettierrc": "jsonc",
    ".textlintrc": "jsonc",
    "zshrc": "shellscript"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.cache": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/.venv": true,
    "**/CVS": true,
    "**/__pycache__": true,
    "**/node_modules": true,
    "**/package-lock.json": false,
    "**/poetry.lock": true,
    "**/yarn.lock": false,
    "*.egg-info": true,
    ".nyc_output": true,
    ".pytest_cache": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/.venv/**": true,
    "**/node_modules/**": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.rebaseWhenSync": true,
  "githubIssues.queries": [
    {
      "label": "Created Issues",
      "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
    },
    {
      "label": "All",
      "query": "state:open repo:${owner}/${repository} sort:created-desc"
    }
  ],
  "githubPullRequests.telemetry.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "html.autoClosingTags": false,
  "javascript.autoClosingTags": false,
  "javascript.preferences.useAliasesForRenames": false,
  "jest.runAllTestsFirst": false,
  "jupyter.alwaysTrustNotebooks": true,
  "jupyter.askForKernelRestart": false,
  "jupyter.maxOutputSize": 800,
  "jupyter.sendSelectionToInteractiveWindow": true,
  "license.author": "Yasuaki Uechi <y@uechi.io>",
  "licenser.author": "Yasuaki Uechi <y@uechi.io>",
  "material-icon-theme.activeIconPack": "react_redux",
  "material-icon-theme.files.associations": {
    "*.LICENSE.txt": "certificate",
    "*.paw": "http",
    ".envrc": "exe",
    ".swiftformat": "swift"
  },
  "material-icon-theme.folders.color": "#938B97",
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.showReloadMessage": false,
  "material-icon-theme.showWelcomeMessage": false,
  "materialTheme.accent": "Indigo",
  "npm.packageManager": "yarn",
  "openInGitHub.useLocalLine": true,
  "platformio-ide.activateOnlyOnPlatformIOProject": true,
  "prettier.packageManager": "yarn",
  "python.analysis.completeFunctionParens": true,
  "python.autoComplete.addBrackets": true,
  "python.diagnostics.sourceMapsEnabled": true,
  "python.formatting.provider": "yapf",
  "python.formatting.yapfArgs": ["--style={based_on_style: google}"],
  "python.languageServer": "Pylance",
  "python.linting.banditEnabled": false,
  "python.linting.flake8Enabled": false,
  "python.linting.pylintEnabled": true,
  "python.pythonPath": "/usr/local/bin/python3",
  "python.showStartPage": false,
  "python.terminal.activateEnvironment": false,
  "ruby.format": "rufo",
  "ruby.useBundler": true,
  "ruby.useLanguageServer": true,
  "scm.autoReveal": false,
  "scm.defaultViewMode": "tree",
  "scm.diffDecorations": "gutter",
  "scm.diffDecorationsGutterWidth": 1,
  "search.exclude": {
    "**/*.code-search": true,
    "**/bower_components": true,
    "**/node_modules": true,
    "**/yarn.lock": true,
    ".venv": true
  },
  "search.seedOnFocus": true,
  "shortcuts.buttons": [
    "terminal , workbench.action.terminal.openNativeConsole, Open in Terminal",
    "file-directory , openInFinder.openRoot , Open in Finder",
    "git-merge , openInGitFork.open , Open in Fork"
  ],
  "tabnine.experimentalAutoImports": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.fontFamily": "Menlo for Powerline, Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.rendererType": "dom",
  "todo.embedded.provider": "rg",
  "todo.file.exclude": [
    "**/.!(todo|todos|task|tasks)/**",
    "**/_output/**",
    "**/bower_components/**",
    "**/build/**",
    "**/dist/**",
    "**/node_modules/**",
    "**/out/**",
    "**/output/**",
    "**/release/**",
    "**/releases/**",
    "**/static/**",
    "**/target/**",
    "**/third_party/**",
    "**/vendor/**",
    "**/.venv/**"
  ],
  "typescript.autoClosingTags": false,
  "typescript.preferences.useAliasesForRenames": false,
  "typescript.suggest.paths": false,
  "vsintellicode.features.python.deepLearning": "enabled",
  "vsintellicode.modify.editor.suggestSelection": "choseToUpdateConfiguration",
  "window.closeWhenEmpty": true,
  "window.newWindowDimensions": "maximized",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Material Theme Ocean High Contrast",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.tabCloseButton": "off",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.sideBar.location": "left",
  "workbench.startupEditor": "welcomePageInEmptyWorkbench"
}
