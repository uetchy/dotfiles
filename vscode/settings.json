        },
        {
          "name": "[HTML] tag name",
          "scope": "entity.name.tag",
          "settings": {
            "foreground": "#bba5d4"
          }
        },
        {
          "name": "[HTML] String inside tag (attribute name)",
          "scope": "meta.tag string",
          "settings": {
            "foreground": "#FBD089"
          }
        },
        {
          "name": "[HTML] tag attribute name",
          "scope": [
            "meta.tag entity.other.attribute-name",
            "entity.other.attribute-name.html"
          ],
          "settings": {
            "foreground": "#987dd9"
          }
        },
        {
          "name": "[HTML] special entity ( &nbsp; &gt; )",
          "scope": [
            "constant.character.entity",
            "punctuation.definition.entity"
          ],
          "settings": {
            "foreground": "#c36586"
          }
        },
        {
          "name": "[Markdown] Header",
          "scope": [
            "entity.name.section.markdown",
            "markup.heading.setext"
          ],
          "settings": {
            "foreground": "#987dd9"
          }
        },
        {
          "name": "[Markdown] List symbol (-, +, *, 1. ...)",
          "scope": "punctuation.definition.list",
          "settings": {
            "foreground": "#987dd9"
          }
        },
        {
          "name": "[Markdown] separator ---",
          "scope": "meta.separator.markdown",
          "settings": {
            "foreground": "#987dd9"
          }
        },
        {
          "name": "[Markdown] inline code `text`",
          "scope": "markup.inline.raw",
          "settings": {
            "foreground": "#bba5d4"
          }
        },
        {
          "name": "[Markdown] bold text **bold**",
          "scope": "markup.bold",
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "name": "[Markdown] italic _italic_",
          "scope": "markup.italic",
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "name": "[Markdown] link description [description](...)",
          "scope": [
            "meta.link punctuation.definition.string",
            "meta.image punctuation.definition.string"
          ],
          "settings": {
            "foreground": "#98939b"
          }
        },
        {
          "name": "[Markdown] link itself [...](LINK)",
          "scope": [
            "link",
            "markup.underline.link",
            "constant.other.reference.link.markdown"
          ],
          "settings": {
            "foreground": "#bcaee5"
          }
        },
        {
          "name": "[Markdown] blockquote text (>)",
          "scope": "markup.quote",
          "settings": {
            "foreground": "#c36586"
          }
        },
        {
          "name": "[CSS] Tag and asterisk",
          "scope": [
            "entity.name.tag.css",
            "entity.name.tag.wildcard"
          ],
          "settings": {
            "foreground": "#FBD089"
          }
        },
        {
          "name": "[CSS] class (.class)",
          "scope": [
            "entity.other.attribute-name.class",
            "entity.other.attribute-name punctuation.definition.entity"
          ],
          "settings": {
            "foreground": "#987dd9"
          }
        },
        {
          "name": "[CSS] pseudo class/element :before :hover",
          "scope": [
            "entity.other.attribute-name.pseudo-element",
            "entity.other.attribute-name.pseudo-class",
            "constant.other.color"
          ],
          "settings": {
            "foreground": "#bba5d4"
          }
        },
        {
          "name": "[CSS] id (#id)",
          "scope": [
            "entity.other.attribute-name.id",
            "entity.other.attribute-name.id punctuation.definition.entity"
          ],
          "settings": {
            "foreground": "#bcaee5"
          }
        },
        {
          "name": "[CSS] numbers",
          "scope": [
            "source.css constant.numeric",
            "source.less constant.numeric",
            "source.scss constant.numeric"
          ],
          "settings": {
            "foreground": "#FBD089"
          }
        },
        {
          "name": "[CSS] property (background-color)",
          "scope": [
            "meta.property-name",
            "support.type.property-name"
          ],
          "settings": {
            "foreground": "#c36586"
          }
        },
        {
          "name": "[Css] property value",
          "scope": [
            "meta.property-value",
            "meta.property-value constant.other",
            "support.constant.property-value"
          ],
          "settings": {
            "foreground": "#bcaee5"
          }
        },
        {
          "name": "[CSS] url",
          "scope": "variable.parameter.url",
          "settings": {
            "foreground": "#c36586"
          }
        },
        {
          "name": "[PHP] php script symbols <?php ?>",
          "scope": [
            "punctuation.section.embedded.begin.php",
            "punctuation.section.embedded.end.php"
          ],
          "settings": {
            "foreground": "#987dd9"
          }
        },
        {
          "name": "[ini] Header",
          "scope": "entity.name.section",
          "settings": {
            "foreground": "#bcaee5"
          }
        },
        {
          "name": "[JSON] property name",
          "scope": "support.type.property-name.json",
          "settings": {
            "foreground": "#987dd9"
          }
        },
        {
          "name": "[Diff] Inserted",
          "scope": "markup.inserted",
          "settings": {
            "foreground": "#96bd65"
          }
        },
        {
          "name": "[Diff] Modified",
          "scope": "markup.changed",
          "settings": {
            "foreground": "#375de6"
          }
        },
        {
          "name": "[Diff] Deleted",
          "scope": "markup.deleted",
          "settings": {
            "foreground": "#F07171"
          }
        },
        {
          "name": "[Diff] Header",
          "scope": "meta.diff.header",
          "settings": {
            "foreground": "#375de6"
          }
        },
        {
          "name": "[Diff] Range",
          "scope": "meta.diff.range",
          "settings": {
            "foreground": "#bba5d4"
          }
        },
        {
          "name": "Italicize",
          "scope": [
            "keyword.control",
            "quote",
            "storage",
            "this",
            "comment",
            "punctuation.definition.comment",
            "modifier"
          ],
          "settings": {
            "fontStyle": "italic"
          }
        }
      ]
    }
  },
  "themeGenerator.italic": true,
  "themeGenerator.tokenIncludeName": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "prettier.requireConfig": true,
  "workbench.colorTheme": "Ancient One Dark - Violet",
  "jupyter.askForKernelRestart": false,
  "terminal.integrated.cursorBlinking": true
}
